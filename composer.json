{
    "name": "james.xue/laravel-emay-sms",
    "description": "Laravel emay sms API",
    "type": "library",
    "license": "MIT",
    "autoload": {
        "psr-4": {
            "Vinhson\\LaravelEmaySms\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Vinhson\\LaravelEmaySms\\Tests\\": "tests/"
        }
    },
    "authors": [
        {
            "name": "xiaoxuan6",
            "email": "1527736751@qq.com"
        }
    ],
    "require-dev": {
        "orchestra/testbench": "^4.0||^5.0||^6.0||^7.0",
        "friendsofphp/php-cs-fixer": "^3.0",
        "laravel/legacy-factories": "1.x-dev",
        "mockery/mockery": "^1.5"
    },
    "require": {
        "php": ">=7.2",
        "ext-json": "*",
        "laravel/framework": "^6.0||^7.0||^8.0||^9.0",
        "guzzlehttp/guzzle": "7.4.x-dev"
    },
    "extra": {
        "laravel": {
            "providers":[
              "Vinhson\\LaravelEmaySms\\LaravelEmaySmsServiceProvider"
            ],
            "aliases":{
                "laravel-emay-sms": "Vinhson\\LaravelEmaySms\\Facades\\LaravelEmaySms"
            }
        }
    },
    "minimum-stability": "dev",
    "scripts": {
        "test": "vendor/bin/phpunit --colors=always",
        "fix-style": "vendor/bin/php-cs-fixer fix --config=.php-cs-fixer.php --using-cache=no --ansi"
    },
    "scripts-descriptions": {
        "test": "Run all tests.",
        "fix-style": "Run style checks and fix violations."
    }
}
